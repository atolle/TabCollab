@model TabRepository.ViewModels.ProjectFormViewModel

@if (Model != null)
{ 
    <h2 hidden id="projectId">@Model.Id</h2>
    <h2 hidden id="projectName">@Model.Name</h2>
}

<table class="table table-bordered table-hover">
    <thead>
        <tr>
            <th width="30%">Description</th>
            <th width="20%">Contributor</th>
            <th width="20%">Date Added</th>
            <th width="10%">Version</th>
            <th width="5%"></th>
            <th width="5%"></th>
            <th width="5%"></th>
            <th width="5%"></th>
        </tr>
    </thead>
    <tbody></tbody>
</table>
